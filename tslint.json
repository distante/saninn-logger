{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["dist", "node_modules", "dist-bundles"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "no-redundant-jsdoc": true,
    "no-namespace": false,
    "ban-types": [false, ["Function"]],
    "interface-name": false,
    "variable-name": false
  }
}
