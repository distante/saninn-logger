{
  "name": "@saninn/logger",
  "version": "0.0.1",
  "private": true,
  "description": "A configurable wrapper around the console object (without losing the console call position)",
  "main": "dist/@saninn__logger.js",
  "types": "dist/@saninn__logger.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-p build:module build:commonjs",
    "build:module": "run-s clean:module && tsc && run-s fix-es2015-modules",
    "build:commonjs": "run-s clean:commonjs && tsc -m commonjs --outDir ./dist/commonjs",
    "build:iife": "npm run clean:iife && rollup -c",
    "clean:module": "shx rm -rf ./dist/es6",
    "clean:commonjs": "shx rm -rf ./dist/commonjs",
    "clean:iife": "shx rm -rf ./dist/iife",
    "fix-es2015-modules": "node ./scripts/fix-es2015-modules.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "console",
    "wrapper",
    "logger"
  ],
  "author": "Saninn Salas Diaz",
  "license": "ISC",
  "homepage": "https://saninnsalas.com",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "filehound": "^1.16.5",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.15.3",
    "pretty-quick": "^1.8.0",
    "rollup": "^1.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "shx": "^0.3.2",
    "tslib": "^1.9.3",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}
